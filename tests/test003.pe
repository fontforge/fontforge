#!/usr/local/bin/fontforge
#Needs: fonts/MunhwaGothic-Bold
# but any other non-otf cid font would do.

Open("fonts/MunhwaGothic-Bold");
Print( "...Read MunhwaGothic-Bold (ps cid)")
Generate("results/MunhwaGothic-Bold.otf");
Print( "...Generated otf (cid)")
Generate("results/MunhwaGothic-Bold.cid");
Print( "...Generated cid")
Close();
Quit()
