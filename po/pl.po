msgid "Accents"
msgstr "Akcenty"

#: prefs.c:440
msgid "Apps"
msgstr "Aplikacje"

#: prefs.c:441
msgid "Font Info"
msgstr "O foncie"

#: prefs.c:442
msgid "Generate"
msgstr "Generuj"

#: prefs.c:443
msgid "PS Hints"
msgstr "Hinting PS"

#: prefs.c:1068 scripting.c:9797
msgid "Call Script"
msgstr "Wykonaj skrypt"

#: prefs.c:1211
msgid "This feature, setting combination is already used"
msgstr "Ta para (funkcja, ustawienie) jest już użyta."

#: prefs.c:1212
msgid ""
"This feature, setting combination is already used\n"
"Do you really wish to reuse it?"
msgstr ""
"Ta para (funkcja, ustawienie) jest już zajęta.\n"
"Czy mimo to kontynuować?"

#: prefs.c:1229 prefs.c:1231
msgid "Tag too long"
msgstr "Zbyt długi tag"

#: prefs.c:1229 prefs.c:1231
msgid "Feature tags must be exactly 4 ASCII characters"
msgstr "Tagi funkcji muszą składać się z dokładnie 4 znaków ASCII"

#: prefs.c:1268 prefs.c:2055
msgid "Mapping"
msgstr "Mapowania"

