#, c-format
msgid "In %s, the advance width (%d) for glyph %s is greater than the maximum (%d)"
msgstr "В %s, ширина (%d) для глифа %s превышает максимальное значение (%d)"

#, c-format
msgid "In GID %d the advance width (%d) is greater than the stated maximum (%d)"
msgstr "В GID %d ширина (%d) больше заданного максимального значения (%d)"

#, c-format
msgid ""
"In GID %d, 'CFF ' advance width (%d) and 'hmtx' width (%d) do not match.\n"
